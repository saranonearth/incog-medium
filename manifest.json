{
    "name": "IncogMedium",
    "version": "1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": [
            "contentScript.js"
        ]
    }],
    "permissions": [
        "activeTab",
        "webNavigation",
        "tabs"
    ]
}