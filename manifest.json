{
    "name": "IncogMedium",
    "description": "Incog Medium is a simple chrome extension which enables you to read articles on Medium endlessly without subscription.",
    "version": "1.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "favicon/s5.png",
            "32": "favicon/s4.png",
            "48": "favicon/s3.png",
            "128": "favicon/s2.png"
        }
    },
    "permissions": [
        "activeTab",
        "webNavigation",
        "tabs"
    ]
}